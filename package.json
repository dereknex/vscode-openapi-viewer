{
    "name": "openapi-viewer",
    "displayName": "openapi-viewer",
    "description": "Viewer documentation for a OpenAPI Specification",
    "icon": "images/icon.png",
    "version": "0.1.9",
    "publisher": "derek-nextty",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dereknex/vscode-openapi-viewer"
    },
    "keywords": [
        "Swagger",
        "OpenAPI",
        "Preview",
        "Documentation",
        "SwaggerPreview"
    ],
    "author": "Derek Chen",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dereknex/vscode-openapi-viewer/issues"
    },
    "homepage": "https://github.com/dereknex/vscode-openapi-viewer#readme",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.previewInSide"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.previewInSide",
                "title": "OpenAPI: Preview"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.48",
        "@types/node": "^7.0.65",
        "eslint": "^4.19.1",
        "typescript": "^2.8.3",
        "vsce": "^1.36.1",
        "vscode": "^1.1.18"
    },
    "dependencies": {
        "js-yaml": "^3.11.0",
        "nunjucks": "^3.1.3",
        "request": "^2.87.0"
    }
}
